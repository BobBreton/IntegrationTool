<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:IntegrationTool.Module.WriteToDynamicsCrm.Logging" xmlns:RecordLogList="clr-namespace:IntegrationTool.Module.WriteToDynamicsCrm.Logging.RecordLogList" x:Class="IntegrationTool.Module.WriteToDynamicsCrm.Logging.LogWindow" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="81*"/>
            <RowDefinition Height="8*"/>
            <RowDefinition Height="211*"/>
        </Grid.RowDefinitions>


        <local:ImportSummaryControl x:Name="ImportSummaryControl"/>

        <RecordLogList:RecordLogListControl x:Name="RecordLogListControl" Grid.Row="2" HorizontalAlignment="Right" Margin="0 0 10 0"/>

    </Grid>
</UserControl>
